{
    "name": "laravel-notification-channels/pushcrew",
    "description": "PushCrew Notifications driver",
    "homepage": "https://github.com/laravel-notification-channels/pushcrew",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bellini",
            "email": "bellini.davide@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/notifications": "5.*|^6.0",
        "illuminate/support": "5.*|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\PushCrew\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\PushCrew\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
