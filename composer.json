{
    "name": "laravel-notification-channels/vwo-engage",
    "description": "VWO Engage Notifications driver",
    "homepage": "https://github.com/laravel-notification-channels/vwo-engage",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bellini",
            "email": "bellini.davide@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/notifications": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Engage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Engage\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
